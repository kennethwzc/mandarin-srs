{
  "name": "mandarin-srs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:all": "pnpm test:ci && pnpm test:e2e",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx lib/db/seed.ts",
    "db:verify": "tsx scripts/verify-database.ts",
    "debug:lessons": "pnpm db:verify && pnpm dev",
    "fix:seed": "pnpm db:push && pnpm db:seed",
    "prepare": "husky install || true",
    "import:content": "tsx scripts/import-content.ts",
    "generate:mnemonics": "tsx scripts/generate-mnemonics.ts",
    "validate:content": "tsx scripts/validate-content.ts",
    "content:reset": "tsx scripts/reset-content.ts",
    "content:backup": "echo \"Implement backup script\"",
    "download:hsk": "tsx scripts/download-hsk-data.ts",
    "generate:hsk:anthropic": "tsx scripts/generate-hsk-with-anthropic.ts",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "echo \"Run: lighthouse http://localhost:3000 --view\"",
    "perf:test": "tsx scripts/performance-test.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.21.1",
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@sentry/nextjs": "^10.30.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-query-devtools": "^5.91.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.31.2",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.394.0",
    "next": "^14.2.3",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.4",
    "posthog-js": "^1.306.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "stripe": "^20.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2",
    "prettier-plugin-tailwindcss": "^0.6.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.22",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.2",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
